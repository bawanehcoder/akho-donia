{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The skeleton application for the Laravel framework.",
    "keywords": [
        "laravel",
        "framework"
    ],
    "license": "MIT",
    "require": {
        "php": "^8.1",
        "ext-curl": "*",
        "artesaos/seotools": "^1.2",
        "asdh/laravel-flatpickr": "^2.0",
        "bezhansalleh/filament-shield": "^3.2",
        "devinweb/laravel-hyperpay": "^1.2",
        "filament/filament": "^3.2",
        "filament/notifications": "3.2",
        "filament/spatie-laravel-media-library-plugin": "3.2",
        "flowframe/laravel-trend": "^0.2.0",
        "flugger/laravel-responder": "^3.3",
        "guzzlehttp/guzzle": "^7.8",
        "hasnayeen/themes": "^3.0",
        "jenssegers/agent": "^2.6",
        "laracasts/flash": "^3.2",
        "laravel/framework": "^10.10",
        "laravel/sanctum": "^3.3",
        "laravel/socialite": "^5.9",
        "laravel/tinker": "^2.8",
        "laravel/ui": "^4.2",
        "maatwebsite/excel": "^3.1",
        "mpdf/mpdf": "^8.2",
        "predis/predis": "^2.2",
        "realrashid/sweet-alert": "^7.1",
        "socialiteproviders/instagram": "^5.0",
        "spatie/laravel-medialibrary": "*",
        "spatie/laravel-permission": "^6.2",
        "spatie/laravel-translatable": "^6.5",
        "spatie/once": "^3.1",
        "synchro/laravel-medialibrary-audio": "*",
        "yajra/laravel-datatables-oracle": "^10.8"
    },
    "require-dev": {
        "fakerphp/faker": "^1.9.1",
        "filament/upgrade": "3.2",
        "laravel/breeze": "^1.29",
        "laravel/pint": "^1.0",
        "laravel/sail": "^1.18",
        "mockery/mockery": "^1.4.4",
        "nunomaduro/collision": "^7.0",
        "phpunit/phpunit": "^10.1",
        "spatie/laravel-ignition": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/",
            "Repositories\\": "Repositories"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi",
            "@php artisan filament:upgrade"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "php-http/discovery": true
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
